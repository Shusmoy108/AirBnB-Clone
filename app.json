{
	"expo": {
		"name": "Airbnb_Clone",
		"slug": "airbnb-clone",
		"description": "An Airbnb clone Expo app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/airbnb-icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/airbnb-splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "mieron.airbnb-clone"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/airbnb-icon.png",
				"backgroundColor": "#000000"
			},
			"package": "com.m_i_e_r_o_n.airbnbclone"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/airbnb-favicon.jpg"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "776e036c-b365-42e2-8959-a1b9e4913074"
			}
		},
		"owner": "m_i_e_r_o_n",
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/15f38312-bee8-433f-a0fa-009de56d0a9f"
		}
	}
}
